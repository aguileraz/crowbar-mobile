<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Export method: app-store, ad-hoc, enterprise, development -->
    <key>method</key>
    <string>app-store</string>
    
    <!-- Team ID for code signing -->
    <key>teamID</key>
    <string>YOUR_TEAM_ID</string>
    
    <!-- Upload symbols for crash reporting -->
    <key>uploadSymbols</key>
    <true/>
    
    <!-- Upload bitcode -->
    <key>uploadBitcode</key>
    <false/>
    
    <!-- Compile bitcode -->
    <key>compileBitcode</key>
    <false/>
    
    <!-- Strip Swift symbols -->
    <key>stripSwiftSymbols</key>
    <true/>
    
    <!-- Thinning for App Store -->
    <key>thinning</key>
    <string>&lt;none&gt;</string>
    
    <!-- Provisioning profiles -->
    <key>provisioningProfiles</key>
    <dict>
        <key>com.crowbar.mobile</key>
        <string>CrowbarMobile_AppStore</string>
    </dict>
    
    <!-- Code signing style -->
    <key>signingStyle</key>
    <string>manual</string>
    
    <!-- Code signing certificate -->
    <key>signingCertificate</key>
    <string>iPhone Distribution</string>
    
    <!-- Destination for export -->
    <key>destination</key>
    <string>export</string>
    
    <!-- Generate app size report -->
    <key>generateAppStoreInformation</key>
    <true/>
    
    <!-- Include manifest for enterprise distribution -->
    <key>manifest</key>
    <dict>
        <key>appURL</key>
        <string>https://example.com/CrowbarMobile.ipa</string>
        <key>displayImageURL</key>
        <string>https://example.com/icon-57.png</string>
        <key>fullSizeImageURL</key>
        <string>https://example.com/icon-512.png</string>
    </dict>
</dict>
</plist>
